{
  "manifest_version": 3,
  "name": "Ai RTPS Magic AutoFill (आय, निवास, जाति)",
  "version": "2.1",
  "description": "Automatically fills RTPS Residence, Caste, and Income Certificate application forms with a single data set.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Fill Certificate Forms"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "resources": ["key-generator.html"],
    "matches": ["<all_urls>"]
  }],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}